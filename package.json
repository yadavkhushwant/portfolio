{
  "name": "portfolio",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://khushwantyadav.me",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:github": "tsc -b && vite build --mode github",
    "build:nginx": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d dist",
    "deploy:github": "npm run build:github && gh-pages -d dist",
    "deploy:nginx": "npm run build:nginx",
    "serve:nginx": "npm run build:nginx && npx serve dist -s"
  },
  "dependencies": {
    "lucide-react": "^0.533.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
